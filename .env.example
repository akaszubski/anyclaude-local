# AnyClaude Environment Variables
# Copy this file to .env and add your actual values
# NOTE: Most configuration now lives in .anyclauderc.json

# GitHub CLI Token (for gh commands - issue creation, PR management)
# Get token from: https://github.com/settings/tokens
# Required scopes: repo, read:org
GH_TOKEN=ghp_your_token_here

# Anthropic API Key (only needed for --mode=claude)
ANTHROPIC_API_KEY=sk-ant-your-key-here

# OpenRouter API Key (only needed for --mode=openrouter)
OPENROUTER_API_KEY=sk-or-v1-your-key-here

# Runtime Overrides (optional - overrides .anyclauderc.json)
# Uncomment to use:
# ANYCLAUDE_MODE=vllm-mlx
# ANYCLAUDE_DEBUG=1
# VLLM_MLX_MODEL=/path/to/different/model
# LMSTUDIO_URL=http://localhost:1234/v1

# Development & Testing
# PROXY_ONLY=true              # Run proxy without Claude Code
# ANYCLAUDE_NO_AUTO_LAUNCH=1   # Disable vLLM-MLX auto-launch
